<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
        http://www.springframework.org/schema/beans/spring-beans.xsd  
        http://code.alibabatech.com/schema/dubbo  
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="prod-sample"/>
	<!--<dubbo:provider timeout="3000" filter="providerRequestFilter"></dubbo:provider>-->
	<dubbo:registry address="zookeeper://172.18.0.20:2181?backup=172.18.0.21:2181,172.18.0.22:2181"></dubbo:registry>

	<dubbo:consumer check="false"  />

	<!--项目系统底层-->
	<!-- 产品&项目 -->
	<dubbo:reference id="projectApiService" interface="com.miaoqian.proj.api.service.ProjectApiService" version="1.0" />
	<dubbo:reference id="productApiService" interface="com.miaoqian.prod.api.service.ProductApiService" version="1.0" />
	<!-- 用户服务 -->
	<dubbo:reference id="outUserApiReadService" interface="com.miaoqian.usersvc.dubbo.api.outuser.OutUserRead" timeout="50000" cache="false"/>
	<dubbo:reference id="outUserApiWriteService" interface="com.miaoqian.usersvc.dubbo.api.outuser.OutUserWrite" timeout="50000" cache="false"/>
	<dubbo:reference id="interUserApiReadService" interface="com.miaoqian.usersvc.dubbo.api.interuser.InterUserRead" timeout="50000" cache="false"/>
	<dubbo:reference id="interUserApiWriteService" interface="com.miaoqian.usersvc.dubbo.api.interuser.InterUserWrite" timeout="50000" cache="false"/>
	<!-- 交易服务 -->
	<dubbo:reference id="frontTradeService" interface="com.miaoqian.trade.api.front.TradeServiceApi" timeout="50000" cache="false"/>


